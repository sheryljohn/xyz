<%= form_for @book do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />

  <%= f.label :author %>:
  <%= f.text_field :author %><br />

  <%= f.label :price %>:
  <%= f.text_field :price %><br />

  <%= f.label :poster_url %>:
  <%= f.text_field :poster_url %><br />

  <%= f.submit %>
<% end %>